{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "compilerOptions": {
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@supabase/supabase-js": "npm:@supabase/supabase-js@^2.49.1",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.61.5",
    "@types/react": "npm:@types/react@^18.3.12",
    "@types/react-dom": "npm:@types/react-dom@^18.3.1",
    "@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@^3.7.1",
    "autoprefixer": "npm:autoprefixer@^10.4.20",
    "axios": "npm:axios@^1.7.8",
    "class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "clsx": "npm:clsx@^2.1.1",
    "dayjs": "npm:dayjs@^1.11.13",
    "immer": "npm:immer@^10.1.1",
    "lucide-react": "npm:lucide-react@^0.462.0",
    "postcss": "npm:postcss@^8.4.49",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "react-icons": "npm:react-icons@^5.5.0",
    "react-router": "npm:react-router@^7.0.1",
    "react-router-dom": "npm:react-router-dom@^7.0.1",
    "react-speech-recognition": "npm:react-speech-recognition@^3.10.0",
    "regenerator-runtime": "npm:regenerator-runtime@^0.14.1",
    "tailwind-merge": "npm:tailwind-merge@^2.5.5",
    "tailwindcss": "npm:tailwindcss@^3.4.15",
    "tailwindcss-animate": "npm:tailwindcss-animate@^1.0.7",
    "ts-pattern": "npm:ts-pattern@^5.6.0",
    "use-immer": "npm:use-immer@^0.11.0",
    "vite": "npm:vite@^6.0.1",
    "vite-plugin-svgr": "npm:vite-plugin-svgr@^4.3.0"
  },
  "nodeModulesDir": "auto"
}